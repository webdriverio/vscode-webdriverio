{
  "name": "vscode-webdriverio",
  "displayName": "vscode-webdriverio",
  "description": "A VSCode extension for WebdriverIO",
  "version": "0.0.1",
  "publisher": "webdriverio",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "webdriverio.showOutput",
        "icon": "$(terminal)",
        "title": "WebdriverIO: Show Extension Output"
      },
      {
        "command": "webdriverio.config-explorer.refreshEntry",
        "icon": "$(refresh)",
        "title": "WebdriverIO: Update Configuration List"
      },
      {
        "command": "webdriverio.testrunner.run",
        "icon": "$(play)",
        "title": "WebdriverIO: Run"
      }
    ],
    "views": {
      "explorer": [],
      "webdriverio": [
        {
          "id": "webdriverio.config-explorer",
          "name": "Configurations",
          "icon": "assets/logo-webdriver-io.svg",
          "contextualTitle": "WebdriverIO Testrunner"
        },
        {
          "id": "webdriverio.reporter-panel",
          "name": "Testrunner",
          "type": "webview",
          "visibility": "visible",
          "icon": "assets/logo-webdriver-io.svg",
          "contextualTitle": "WebdriverIO Testrunner"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "webdriverio",
          "title": "WebdriverIO Testrunner",
          "icon": "assets/logo-webdriver-io.svg"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "webdriverio.configFileEditor",
        "displayName": "WebdriverIO Configuration",
        "selector": [
          {
            "filenamePattern": "wdio.*.js"
          },
          {
            "filenamePattern": "wdio.*.ts"
          }
        ]
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "webdriverio.testrunner.run",
          "when": "viewItem == wdioConfig",
          "group": "inline"
        },
        {
          "command": "webdriverio.config-explorer.refreshEntry",
          "when": "viewItem == wdioConfig",
          "group": "inline"
        },
        {
          "command": "webdriverio.testrunner.run",
          "when": "viewItem == wdioSuite",
          "group": "inline"
        }
      ]
    }
  },
  "icon": "assets/webdriverio.svg",
  "galleryBanner": {
    "color": "#ea5906",
    "theme": "light"
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "run-s build:*",
    "build:ts": "tsc -p ./",
    "build:rollup": "npx rollup --config rollup.config.mjs",
    "dev": "npm run watch",
    "watch": "run-p watch:*",
    "watch:ts": "npm run build:ts -- --watch",
    "watch:rollup": "npm run build:rollup -- --watch",
    "pretest": "npm run build && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash.pick": "^4.4.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.1",
    "@types/serialize-javascript": "^5.0.2",
    "@types/vscode": "^1.80.0",
    "@types/workerpool": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/test-electron": "^2.3.3",
    "eslint": "^8.44.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@bendera/vscode-webview-elements": "^0.14.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jscodeshift": "^0.11.6",
    "@types/node-ipc": "^9.2.0",
    "@types/vscode-webview": "^1.57.1",
    "@vscode/codicons": "0.0.33",
    "@wdio/cli": "^8.12.2",
    "@wdio/reporter": "^8.12.2",
    "@wdio/types": "^8.10.4",
    "eta": "^3.0.3",
    "jscodeshift": "^0.15.0",
    "lodash.pick": "^4.4.0",
    "node-ipc": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.26.2",
    "serialize-javascript": "^6.0.1",
    "tangle": "^3.0.0",
    "ts-node": "^10.9.1",
    "workerpool": "^6.4.0"
  }
}
